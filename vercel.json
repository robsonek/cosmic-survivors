{
  "buildCommand": "npm run build:deploy",
  "outputDirectory": "dist",
  "framework": null,
  "rewrites": [
    {
      "source": "/game/(.*)",
      "destination": "/game/$1"
    }
  ],
  "headers": [
    {
      "source": "/game/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        },
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}
